# AWX Configuration Environment Variables
# Copy this file to .env and customize for your environment

# DUT (Device Under Test) Configuration
DUT_HOSTNAME=192.168.1.10
DUT_USERNAME=ec2-user

# Load Generator Configuration
LOADGEN_LOCAL=true  # Set to 'false' if using remote load generator
# LOADGEN_HOSTNAME=192.168.1.20  # Only needed if LOADGEN_LOCAL=false

# SSH Key Path (for DUT access)
SSH_KEY_PATH=~/.ssh/id_rsa

# AWX Connection (automatically set based on deployment type)
# KIND: http://localhost:30080
# Docker Compose: http://localhost:8052
CONTROLLER_HOST=http://localhost:30080
CONTROLLER_USERNAME=admin
CONTROLLER_PASSWORD=password  # pragma: allowlist secret

# Git Repository (customize if using a fork)
PROJECT_SCM_URL=https://github.com/maryamtahhan/vllm-cpu-perf-eval.git
PROJECT_SCM_BRANCH=awx-integration
